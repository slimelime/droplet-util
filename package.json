{
    "name": "@myob/droplet-util",
    "version": "1.0.1",
    "description": "Utilities for Droplet",
    "main": "index.js",
    "engines": {
        "node": "8.10"
    },
    "scripts": {
        "clean": "rm -rf node_modules; rm -rf ./lib; rm -rf ./dist",
        "test": "jest ./src --verbose",
        "build": "babel --copy-files --out-dir lib --ignore *.spec.js,__mocks__ src",
        "build-dev": "yarn --dev; npm run build",
        "build-prod": "yarn --prod; npm run build",
        "clean-build-dev": "npm run clean; npm run build-dev",
        "clean-build-prod": "npm run clean; npm run build-prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MYOB-Technology/droplet-util.git"
    },
    "author": "Tranquility team",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MYOB-Technology/droplet-util/issues"
    },
    "homepage": "https://github.com/MYOB-Technology/droplet-util#readme",
    "dependencies": {
        "@myob/droplet-slo": "^3.0.1",
        "@myob/inbound-idam-auth": "^3.0.1",
        "adm-zip": "^0.4.7",
        "ajv": "^5.5.2",
        "argparse": "^1.0.9",
        "aws-sdk": "^2.258.1",
        "aws-xray-sdk-core": "^1.2.0",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "babel-runtime": "^6.26.0",
        "csv-parse": "^1.2.0",
        "dynamo-update-expression": "^1.0.8",
        "fs-extra": "^4.0.2",
        "inquirer": "^5.0.1",
        "json2csv": "^3.11.0",
        "jsonpath": "^0.2.11",
        "limiter": "^1.1.2",
        "lodash": "4.17.4",
        "md5.js": "^1.3.4",
        "memorystream": "^0.3.1",
        "moment": "^2.21.0",
        "moment-timezone": "^0.5.14",
        "pg": "~7.1.2",
        "pg-escape": "^0.2.0",
        "promise-retry": "^1.1.1",
        "readable-stream": "^2.3.3",
        "rxjs": "^5.4.1",
        "rxjs-stream": "^1.1.0",
        "superagent": "^3.8.2",
        "through2": "^2.0.3",
        "traverse": "^0.6.6",
        "uuid": "^3.0.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-plugin-transform-async-generator-functions": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2017": "^6.24.1",
        "eslint": "~3.18.0",
        "jest-cli": "19.0.2",
        "nock": "^9.2.5",
        "ramda": "^0.24.1",
        "repl": "^0.1.3",
        "transducers-js": "^0.4.174"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/lib/",
            "<rootDir>/node_modules/"
        ]
    }
}
