{
  "presets": [
    ["env", {
      "targets": {
        "node": "6.10"
      },
      "debug": true
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }],
    ["transform-builtin-extend", {
      "globals": ["Error"]
    }],
    "transform-async-generator-functions"
  ]
}
